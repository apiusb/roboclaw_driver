<launch>
   <node respawn="false" name="roboclaw_driver" pkg="roboclaw_driver" type="roboclaw_driver" output="screen" >
		<param name="port" value="/dev/ttyACM0" />
		<param name="baud_rate" value="1000000" />
		<param name="rate" value="20" />
		<param name="base_width" value="0.314" />
		<param name="ticks_per_metre" value="21738" />
		<param name="base_frame_id" value="base_link" />
		<param name="QPPS" value="11600" />
		<param name="timeout" value="2" />
   </node>
</launch>

