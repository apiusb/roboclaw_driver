<launch>
   <node name="roboclaw_driver" pkg="roboclaw_driver" type="roboclaw_driver" output="screen" launch-prefix="gdb -ex run --args">
		<param name="port" value="/dev/ttyACM0" />
		<param name="baud_rate" value="1000000" />
		<param name="rate" value="30" />
		<param name="base_width" value="0.195" />
		<param name="ticks_per_metre" value="165.786" />
		<param name="base_frame_id" value="base_link" />	
		<param name="timeout" value="20" />
   </node>
</launch>
